    "d3-transition": "1",
    "hammerjs": "2",
    "lodash-es": "4"
  },
  "devDependencies": {
    "eslint": "5.10.0",
    "jsdom": "13.1.0",
    "rollup": "0.68.1",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-terser": "^5.1.1",
    "tape": "4.9.1"
  },
  "browserslist": "> 0.25%, not dead"
